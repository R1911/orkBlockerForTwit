{
  "manifest_version": 3,
  "name": "Twitter Softblocker",
  "version": "1.0",
  "description": "Removes users defined from feed.",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
